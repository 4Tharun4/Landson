generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Products{
id String @id @default(auto()) @map("_id") @db.ObjectId
ProductDesciption String
ProductEfficiency  String 
ProductEngineoilcapacity String
ProductFueltankcapacity String
ProductMaxpower String
ProductModel String
ProductRPM String
ProductTitle String
imageUrl String




}
model DealersData{
    id String @id @default(auto()) @map("_id") @db.ObjectId
  DealerName String
  DealerNumber String
  DealerEmail String @unique
  DealerAddress String
  DealerId  String
  PassWord String @default("Landson123")
  role String @default("DEALER")

 }
 model UsersData{
      id String @id @default(auto()) @map("_id") @db.ObjectId
      Name String
      Email String
     PhoneNumber String
     Password String
     Address String
     isVerfied String @default("false")
     role String @default("USER")
 }